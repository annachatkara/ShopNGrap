{
  "name": "flutter-app-backend",
  "version": "1.0.0",
  "description": "Backend API for Flutter application using Node.js, Express, and Prisma",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node migrate.js",
    "db:init": "node migrate.js init",
    "db:generate": "npx prisma generate",
    "db:migrate": "node migrate.js migrate",
    "db:deploy": "node migrate.js deploy",
    "db:reset": "node migrate.js reset",
    "db:studio": "node migrate.js studio",
    "db:seed": "node migrate.js seed",
    "db:status": "node migrate.js status",
    "lint": "eslint . --ext .js",
    "test": "jest",
    "test:watch": "jest --watch",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log"
  },
  "keywords": ["nodejs", "express", "prisma", "postgresql", "flutter", "api", "backend"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "@prisma/client": "^5.6.0",
    "prisma": "^5.6.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "@types/node": "^20.9.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
